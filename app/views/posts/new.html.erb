<h2> Add a new post</h2>
<div class="new-post">
<%= form_with model: @post,  url: new_post_path do |form| %>
    <div>
      <%= form.label :title %>
      <%= form.text_field :title %>
    </div>
    <div>
      <%= form.label :text %>
      <%= form.text_area :text %>
    </div>
    <%= form.submit "Save" %>
  <% end %>
</div>
